#!/bin/sh

export GTK_IM_MODULE=fcitx
export QT_IM_MODULE=fcitx
export XMODIFIERS=@im=fcitx
fcitx &

/usr/lib/notification-daemon-1.0/notification-daemon &

feh --bg-fill --randomize ~/.wallpaper/* &

xset r rate 300 50 &

while true
do
  xsetroot -name "$(date '+  %Y-%m-%d  %H:%M:%S (%a)  ')"
  sleep 1s
done &

exec dwm

